{
    "name": "zunky44/laravel-repository-pattern",
    "description": "Repository for Laravel 5.*",
    "keywords": ["laravel", "repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagat Karki",
            "email": "jagat.kc34@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/cache": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/events": "^5.1",
        "illuminate/pagination": "^5.1",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7",
        "mockery/mockery": "0.9.*|~1.0",
        "nesbot/carbon": "~1.20",
        "phpunit/phpunit": "~4.8|~5.4|~6.1|~7.0"
    },
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "zunky44\\laravel-repository-pattern\\": "src/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "zunky44\\laravel-repository-pattern\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
